#include ".config/colors/shiro.xresources"

! XFT Settings {{{
!Xft.autohint: false
!Xft.antialias: false
!Xft.hinting: falst
!Xft.hintstyle: hintslight
!Xft.rgba: rgba
!Xft.lcdfilter: lcddefault
!Xft.dpi: 96
! }}}
! URxvt {{{
!URxvt*dpi: 96
!URxvt*antialias: true
!URxvt*rgba: vrgb
!URxvt*hinting: true
!URxvt*hintstyle: hintslight

URxvt*depth: 32
URxvt*font: xft:Tewi:regular:pixelsize=6:antialias=false:hinting=false:autohint=false
URxvt*boldFont:
URxvt*italicFont:
URxvt*bolditalicFont:
URxvt*lineSpace: 3
URxvt*letterSpace: 2
URxvt*internalBorder: 20
URxvt*pointerBlank: true
URxvt*cursor: i-beam
URxvt*cursorBlink: true
URxvt*cursorUnderline: true
!URxvt*geometry: 95x25
URxvt*url-launcher: /usr/bin/chromium
URxvt*perl-lib: /usr/lib/urxvt/perl/
URxvt*perl-ext-common: default,matcher
URxvt*matcher.button: 1
URxvt*cutchars: "\\ `\"\'()*;<>[]{|}&,=?@^\:"
URxvt*iso14755: false
URxvt*iso14755_52: false
URxvt*meta8: false
URxvt*scrollBar: false
URxvt*keysym.Control-Up: \033[1;5A
URxvt*keysym.Control-Down: \033[1;5B
URxvt*keysym.Control-Left: \033[1;5D
URxvt*keysym.Control-Right: \033[1;5C
URxvt*keysym.M-c: perl:clipboard:copy
URxvt*keysym.M-v: perl:clipboard:paste
! }}}
! Misc Applications {{{
rofi.color-enabled: true
rofi.color-normal: #ffffff,#525d76,#f5f5f5,#df3f3f,#ffffff
rofi.color-urgent: #ffffff,#dc322f,#f5f5f5,#df3f3f,#dc322f
rofi.color-active: #ffffff,#c2cad0,#f5f5f5,#df3f3f,#c2cad0
rofi.color-window: #ffffff,#f5f5f5
rofi.separator-style: solid
rofi.font: tewi 6
rofi.padding: 5
rofi.lines: 5
rofi.borderwidth: 0
! }}}
! vim: et sw=2 syn=xdefaults
