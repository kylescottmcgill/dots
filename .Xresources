! XFT Settings {{{

Xft.autohint: true
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter: lcddefault
Xft.dpi: 96

! }}}

! URxvt {{{

urxvt*dpi:        96
urxvt*antialias:  true
urxvt*rgba:       vrgb
urxvt*hinting:    true
urxvt*hintstyle:  hintslight

urxvt*geometry:                 80x25
urxvt*foreground:               #CCCCCC
urxvt*depth:                    32
urxvt*scrollBar:                false
urxvt*font:                     xft:tewi-8
urxvt*internalBorder:           20
urxvt*colorBD:                  #CDCDAD
urxvt*cursor:                   i-beam
urxvt*cursorBlink:              true
urxvt*cursorUnderline:          true
urxvt*urlLauncher:              /usr/bin/firefox-developer
urxvt*perl-lib:                 /usr/lib/urxvt/perl/
urxvt*perl-ext-common:          default,matcher
urxvt*matcher.button:           1
urxvt*cutchars:                 `""()''*<>[]{|}
!urxvt*background:              #1E231F
!urxvt*borderLess:              true
!urxvt*depth: 32
!urxvt*background: rgba:0000/0000/0200/dddd
urxvt*background: rgba:0000/0000/0200/b800

! #Black
urxvt.color0: #36393a
urxvt.tolor8: #627369

! #Red
urxvt.color1: #924445
urxvt.color9: #803536

! #Green
urxvt.color2: #456140
urxvt.color10: #33642A

! #Yellow
urxvt.color3: #DAB257
urxvt.color11: #AC8E46

! #Blue
urxvt.color4: #657386
urxvt.color12: #334E72

! #Magenta
urxvt.color5: #977F94
urxvt.color13: #7A6A78

! #Cyan
urxvt.color6: #7DB9BA
urxvt.color14: #5F9898

! #White
urxvt.color7: #d7d9cd
urxvt.color15: #f6f6f5

! }}}

! Misc Applications {{{

!Xcursor*theme: Simple-and-Soft
!Xcursor.size: 22

rofi.background: #1F2833
rofi.foreground: #d2d2ca
rofi.highlightbg: #151B22
rofi.highlightfg: #924445
rofi.bordercolor: #333333
rofi.font: tewi 8
rofi.padding: 5
rofi.lines: 5
rofi.borderwidth: 0
!rofi.terminal: termite
!rofi.opacity: 90
!rofi.width: 50

! }}}


