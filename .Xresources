! General -----------------------------------------
xterm*termName: xterm-256color
xterm*locale: true

Xft.autohint: true
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter: lcddefault
Xft.dpi: 96

URxvt*termName: rxvt-256color
URxvt*loginShell: false
URxvt*saveLines: 1024
URxvt*geometry: 80x20
URxvt*font: xft:Terminus:pixelsize=12
URxvt*boldFont: xft:Terminus:pixelsize=12
URxvt*scrollBar: false
URxvt*cursorBlink: true
URxvt*internelBorder: 10
URxvt-white*internalBorder: 20
! URxvt*perl-ext-common:      default,url-select,keyboard-select
! URxvt.url-select.launcher:  /usr/bin/dwb
! URxvt.url-select.launcher:  /usr/bin/urxvt -e elinks
! URxvt.url-select.underline: true
! URxvt.keysym.M-u:           perl:url-select:select_next
! URxvt.keysym.M-v:           perl:keyboard-select:activate

