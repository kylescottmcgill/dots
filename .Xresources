! XFT Settings {{{

Xft.autohint: true
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter: lcddefault
Xft.dpi: 96

! }}}
! URxvt {{{

URxvt*dpi: 96
URxvt*antialias: true
URxvt*rgba: vrgb
URxvt*hinting: true
URxvt*hintstyle: hintslight

URxvt*geometry: 800x600
URxvt*depth: 32
URxvt*scrollBar: false
URxvt*font: xft:tewi-8
URxvt*lineSpace: 3.5
URxvt*internalBorder: 20
URxvt*colorBD: #FFFFFF
URxvt*cursor: i-beam
URxvt*cursorBlink: true
URxvt*cursorUnderline: true
URxvt*url-launcher: /usr/bin/chromium
URxvt*perl-lib: /usr/lib/urxvt/perl/
URxvt*perl-ext-common: default,matcher
URxvt*matcher.button: 1
URxvt*cutchars: `""()''*<>[]{|}
URxvt*iso14755: false
URxvt*iso14755_52: false
URxvt*keysym.Control-Up: \033[1;5A
URxvt*keysym.Control-Down: \033[1;5B
URxvt*keysym.Control-Left: \033[1;5D
URxvt*keysym.Control-Right: \033[1;5C
URxvt*foreground: #FFFFFF
!URxvt*background: #282A31
!Slightly Lighter
!URxvt*background: #34343E
!Slightly Vim-Gotham
URxvt*background: #13232F

! #Black
URxvt*color0: #36393a
URxvt*tolor8: #627369

! #Red
URxvt*color1: #924445
URxvt*color9: #803536

! #Green
URxvt*color2: #456140
URxvt*color10: #33642A

! #Yellow
URxvt*color3: #DAB257
URxvt*color11: #AC8E46

! #Blue
URxvt*color4: #657386
URxvt*color12: #334E72

! #Magenta
URxvt*color5: #977F94
URxvt*color13: #7A6A78

! #Cyan
URxvt*color6: #7DB9BA
URxvt*color14: #5F9898

! #White
URxvt*color7: #d7d9cd
URxvt*color15: #f6f6f5

! }}}
! Misc Applications {{{

Xcursor*theme: Paper
Xcursor.size: 22

rofi.background: #1F2833
rofi.foreground: #d2d2ca
rofi.highlightbg: #151B22
rofi.highlightfg: #924445
rofi.bordercolor: #333333
rofi.font: tewi 8
rofi.padding: 5
rofi.lines: 5
rofi.borderwidth: 0
!rofi.terminal: termite
!rofi.opacity: 90
!rofi.width: 50

! }}}

