! XFT Settings {{{

Xft.autohint: true
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter: lcddefault
Xft.dpi: 96

! }}}
! URxvt Settings {{{

URxvt*loginShell: false
URxvt*saveLines: 1000
URxvt*geometry: 80x20
URxvt*termName: rxvt-256color

!URxvt*font: xft:terminus:pixelsize=12:antialias=true:hinting=true
!URxvt*boldfont: xft:terminus:bold:pixelsize=12:antialias=true:hinting=true
!
URxvt*font: -*-tewi-medium-*-*--11-*-*-*-*-*-*-1
URxvt*boldFont: -*-tewi-medium-*-*--11-*-*-*-*-*-*-1

URxvt*scrollBar: false
URxvt*cursorBlink: true
URxvt*iso14755: false
URxvt*iso14755_52: false
URxvt*internalBorder: 10
URxvt.depth: 0
URxvt.transparent: false
!URxvt.shading: 5

URxvt-white.transparent: false

URxvt-white*internalBorder: 20

URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select,matcher
URxvt.url-select.launcher:  chromium
URxvt.url-select.underline: true
URxvt.matcher.button:       1
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search
URxvt*keysym.M-c:           perl:clipboard:copy
URxvt*keysyn.M-v:           perl:clipboard:paste

URxvt*keysym.M-Down:        \033[1;3B
URxvt*keysym.M-Up:          \033[1;3A
URxvt*keysym.M-Left:        \033[1;3D
URxvt*keysym.M-Right:       \033[1;3C

URxvt*keysym.Control-Up:     \033[1;5A
URxvt*keysym.Control-Down:   \033[1;5B
URxvt*keysym.Control-Left:   \033[1;5D
URxvt*keysym.Control-Right:  \033[1;5C

! }}}
! Colors {{{

! URxvt
!#include ".colors/default.xresources"

#include ".colors/lol.xresources"
!#include ".colors/blushsh.xresources"
!#include ".colors/vypr.xresources"
!#include ".colors/base16-grayscale.dark.256.xresources"
!#include ".colors/base16-grayscale.light.256.xresources"
!#include ".colors/TomorrowNight.xresources"

! URxvt-white
#include ".colors/z-brah-white.xresources"

! }}}
! Misc Applications {{{

Xcursor*theme: Simple-and-Soft
Xcursor.size: 22

rofi.background: #1F2833
rofi.foreground: #d2d2ca
rofi.highlightbg: #151B22
rofi.highlightfg: #924445
rofi.bordercolor: #333333
rofi.font: tewi 14
rofi.padding: 700
rofi.lines: 5
rofi.borderwidth: 0
rofi.terminal: urxvtc
rofi.opacity: 90
rofi.width: 100

Skype*background: #f6f6f6
Skype*foreground: #888888

! }}}
