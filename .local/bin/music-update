#!/bin/bash

cd ~/Music/ && \
	youtube-dl --extract-audio --audio-format=mp3 -f 140 \
		https://www.youtube.com/playlist?list=PLiFU_tw8Vf8xP4tbqaamdnw87dkBGkfi4 ;

cd ~/Music/ && \
	youtube-dl --extract-audio --audio-format=mp3 -f 140 \
		https://soundcloud.com/monstercat/ ;

